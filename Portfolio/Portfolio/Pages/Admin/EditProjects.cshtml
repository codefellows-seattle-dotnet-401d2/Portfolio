@page "{id}"

@model Portfolio.Pages.Admin.EditProjectsModel


@{
    Layout = "_Layout";
    ViewData["Title"] = "EditProjects";
}

<h2>Edit Projects</h2>


<form method="post">
    <div class="form-group">
        <label asp-for="Title" class="col-md-2" control-label></label>
    </div>

    <label asp-for="Image" class="col-md-3" control-label> </label>
    <input type="file" asp-for="Image">


    <button type="submit" class="btn btn-primary">Save</button>
    <button type="submit">Delete</button>

</form>

